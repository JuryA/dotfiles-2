#!/bin/sh
. ~/.xinitrc.local
sh ~/.fehbg &
killall ssh-agent
rm -f ~/.ssh/ssh_auth_sock
setxkbmap 'us(colemak),ru' -option -option ctrl:nocaps -option grp:win_space_toggle
xset r rate 210 35
xsetroot -cursor_name left_ptr
xrdb ~/.Xresources &
dunst &
compton -bc -o 0.35 --shadow-exclude 'n:e:gromit-mpx'
exec xmonad
