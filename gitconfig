[github]
    user = myfreeweb
[difftool "Kaleidoscope"]
    cmd = ksdiff-wrapper git \"$LOCAL\" \"$REMOTE\"
[difftool]
    prompt = false
[diff]
    renames = copy
  	tool = vimdiff
  	guitool = Kaleidoscope
[color]
    ui = true
[alias]
    a = add -A
    ass = add -A # LOL https://gist.github.com/894971
    ci = commit -a
    st = status
    tags = tag -n1 -l
    rwt = reset --hard HEAD
    unci = reset --hard HEAD^
    sup = submodule foreach --recursive git pull origin master
    ksdiff = difftool --gui
[url "git://github.com/"]
    insteadOf = gh:
[url "git@github.com:myfreeweb/"]
    insteadOf = my:
[core]
  	excludesfile = ~/Dropbox/dotfiles/gitignore
